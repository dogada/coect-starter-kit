    "coect": "latest",
    "coect-admin": "latest",
    "coect-account": "latest",
    "coect-umedia": "latest",
    "connect-redis": "~3.0.2",
    "cookie-parser": "~1.4.1",
    "debug": "~2.2.0",
    "del": "~2.2.0",
    "edid": "~0.1.2",
    "ejs": "~2.4.1",
    "errorhandler": "~1.4.3",
    "express": "~4.13.4",
    "express-flash": "0.0.2",
    "express-session": "~1.13.0",
    "express-validator": "~2.20.3",
    "font-awesome": "~4.6.1",
    "ioredis": "~1.15.1",
    "jquery": "~2.2.3",
    "knex": "~0.10.0",
    "linkify-it": "~1.2.0",
    "lodash": "~3.10.1",
    "lusca": "~1.3.0",
    "method-override": "~2.3.5",
    "moment": "~2.13.0",
    "morgan": "~1.7.0",
    "object-assign": "~4.0.1",
    "page": "~1.7.1",
    "pg": "~4.5.3",
    "pmx": "~0.6.1",
    "request": "~2.72.0",
    "riot": "2.3.18",
    "secure-filters": "~1.0.5",
    "serialize-javascript": "~1.3.0",
    "serve-favicon": "~2.3.0",
    "tflow": "latest",
    "toobusy-js": "~0.4.3",
    "wpml": "latest"
  },
  "repository": {
    "type": "git",
    "url": "ssh://web0.matrix/srv/repo/mysite_site.git/"
  },
